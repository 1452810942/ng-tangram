  "scripts": {
    "start": "npm run dev",
    "build": "node build.js",
    "build:docs": "npm run docs:browser && npm run docs:server",
    "prebuild": "rm -rf dist && rm -rf out-tsc",
    "docs:browser": "webpack --progress --env.aot --env.browser -p",
    "docs:server": "webpack --progress --env.aot --env.server",
    "predocs:browser": "rimraf docs/browser",
    "predocs:server": "rimraf docs/server",
    "dev": "webpack-dev-server",
    "server": "node docs/server/main.bundle",
    "publish": "cd dist/@ng-tangram/animate && npm publish && cd ../components && npm publish && cd ../../../"
  },
