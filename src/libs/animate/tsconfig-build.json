{
  "compilerOptions": {
    "declaration": true,
    "stripInternal": false,
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "importHelpers": true,
    "newLine": "lf",
    "module": "es2015",
    "moduleResolution": "node",
    "outDir": "../../../out-tsc/@ng-tangram/animate/",
    "rootDir": ".",
    "rootDirs": [
      ".",
      "../../../dist/@ng-tangram/animate/"
    ],
    "sourceMap": true,
    "inlineSources": true,
    "target": "es2015",
    "lib": ["es2015", "dom"],
    "skipLibCheck": true,
    "types": [],
    "baseUrl": ".",
    "paths": {
      "@ng-tangram/animate/*": ["../../../dist/@ng-tangram/animate/*"]
    }
  },
  "files": [
    "./public-api.ts"
  ],
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "strictMetadataEmit": false,
    "skipTemplateCodegen": true,
    "flatModuleOutFile": "animate.js",
    "flatModuleId": "@ng-tangram/animate",
    "fullTemplateTypeCheck": true
  }
}
