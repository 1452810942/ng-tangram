{
  "compilerOptions": {
    "declaration": true,
    "stripInternal": false,
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "newLine": "lf",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "inlineSources": true,
    "target": "es2015",
    "lib": ["es2018", "dom"],
    "skipLibCheck": true
  }
}
